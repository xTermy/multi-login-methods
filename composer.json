{
    "name": "stormcode/multi-login-methods",
    "description": "Simple package that allows to make classes for multiple login methods",
    "keywords": [
        "stormcode",
        "laravel",
        "multi-login-methods"
    ],
    "license": "MIT",
    "homepage": "https://github.com/xTermy/multi-login-methods",
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.17",
        "laravel/framework": "^11"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "autoload": {
        "psr-4": {
            "StormCode\\MultiLoginMethods\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StormCode\\MultiLoginMethods\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Miko≈Çaj Salamak",
            "email": "mikolaj.salamak@stormcode.pl"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "StormCode\\MultiLoginMethods\\MultiLoginMethodsServiceProvider"
            ]
        }
    }
}
